from typing import List
class Solution:
    def search(self, nums: List[int], target: int) -> int:
        l, r = 0, len(nums) - 1

        sorted(nums)
        while l < r:
            m = l + (r - l) // 2
            print("m = ", m, " l = ", l, " r = ", r)
            if nums[m] == target:
                return m
            
            if target < nums[m]:
                r = m
            else:
                l = m
            
        return -1
    
l, r = 4, 9
print(l + (r - l) // 2)